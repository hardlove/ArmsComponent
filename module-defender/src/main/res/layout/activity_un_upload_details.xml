<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/color_FFFFFF"
              android:orientation="vertical">

    <com.hardlove.mobiledf.widgets.CustomToolBar
        android:id="@id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/colorPrimary"
        app:leftIcon="@mipmap/nav_icon_back"
        app:leftTitle="未上传结果详情"
        app:rightIcon="@mipmap/nav_icon_confirm"
        app:rightSubIcon="@mipmap/nav_icon_print">
    </com.hardlove.mobiledf.widgets.CustomToolBar>

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <include
            layout="@layout/include_un_upload_details"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />

    </ScrollView>


</LinearLayout>